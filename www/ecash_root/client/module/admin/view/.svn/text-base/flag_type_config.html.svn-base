<link rel="stylesheet" type="text/css" href="lib/ext/resources/css/ext-all.css"  />
<style type="text/css">
	
	.add_icon .x-btn-text
	{
		background-image: url(image/add.gif);
	}
	.add_rule_icon .x-btn-text
	{
		background-image: url(image/chart_organisation_add.gif);
	}
	.rule_box
	{
		margin: 5px 5px 0px 5px;
	}
	.rule_box .x-tree
	{
		margin: 0px 5px 5px 5px;
	}
	.rule_box .x-tree-node-el
	{
		background-color: #EBF3FD;
		border: #C9D5F7 solid 1px;
	}
	.rule_panel
	{
		border: #98C0F4 solid 1px;
		padding: 0px 2px 2px 2px;
		margin-bottom: 10px;
		background-color: #EBF3FD;
		
	}
	.rule_layout
	{
		margin-bottom: 2px;
		background-color: #EBF3FD;
		height: 500px;
		position:relative;
	}
	
	.white_box, .rule_table
	{
		background-color: #FFFFFF;
		border: #C9D5F7 solid 1px;
		padding: 2px;
		margin: 2px 0px 0px 0px;
	}
	.rule_table
	{
		width: 100%;
	}
	.rule_table td
	{
		font-size: 11px;
	}
	.rule_name
	{
		width: 300px;
	}
	.selection_form_panel
	{
		width: 400px;
	}
	.view_basic
	{
		cursor: pointer;
	}
	.light_blue_bg
	{
		background-color: #EBF3FD;
		border: #CFE5FF solid 1px;
	}
	.main_layout
	{
		position:relative;
		/*margin-top:60px;*/
		height: 500px;
	}
	.condition_form
	{
		display: none;
	}
	.action_form
	{
		visibility: hidden;
	}
	.drop_target
	{
		padding: 1px;
	}
	.drop_target_over
	{
		background-color: #E0ECFC;
		padding: 2px;
	}
	div.flag_type_select
	{
		padding-left:2px;
	}
</style>
<script type="text/javascript" src="lib/ext/adapter/ext/ext-base.js"></script>
<script type="text/javascript" src="lib/ext/ext-all-debug.js"></script>
<script src="/js/flag_type_config.js" language="javascript" type="text/javascript"></script>
<div id="page_container"></div>
<script type="text/javascript">
	TSS.eCash.Config.flag_types = new Ext.data.SimpleStore({fields: ['date_modified', 'date_created', 'active_status', 'flag_type_id', 'name', 'name_short'], data: %%%flag_type_json%%%});
	TSS.eCash.Config.flag_types.sort('name_short');
	TSS.eCash.Config.message = '%%%result%%%';
	TSS.eCash.Config.init('page_container');
	TSS.eCash.Config.flag_type_record = Ext.data.Record.create(
		    {name: 'date_modified'},
		    {name: 'date_created'},
		    {name: 'active_status'},
		    {name: 'flag_type_id'},
		    {name: 'name'},
		    {name: 'name_short'}
		);

</script>
