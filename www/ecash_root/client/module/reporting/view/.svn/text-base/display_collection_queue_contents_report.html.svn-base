<form name="report_form" class="no_padding" action="/?module=reporting&action=generate_report" method="post" onSubmit="document.getElementById('loading').style.visibility = 'visible';">
<table width="100%" border="0" style='background: #EEEEEE' cellpadding="0" cellspacing="0">
	<tr class="valign_top">
		<th>
			%%%report_title%%%
		</th>
	</tr>
	<tr class="valign_top">
		<td align="left">
			<table width="780" border="0">
				<tr>
					<td style="text-align: right"  valign="top" width="10%">
						%%%start_date_title%%%
					</td>
					<td style="text-align: left" valign="top" align="left" width="25%">
						%%%start_date%%%
					</td>
					<td valign="top" align="center" width="30%">
						
					</td>					
					<td style="text-align: right" valign="top" align="right" width="35%">
						%%%company_select_list%%%
					</td>
				</tr>
				<tr>
					<td style="text-align: right" valign="top">
						%%%end_date_title%%%
					</td>
					<td style="text-align: left" valign="top" align="left">
						%%%end_date%%%
					</td>
					<td valign="top"  align="center" nowrap>
						<input type="submit" class="button" value="Submit">
						<input type="hidden" name=mode value="%%%report_mode%%%">
						&nbsp;
						<input type="button" class="button" value="Refresh" onclick="reset();submit();">
					</td>
					<td style="text-align: right" valign="top" align="right" nowrap>%%%queue_name_list%%%</td>
				</tr>
			</table>
		</td>
	</tr>
	<tr>
		<td>
		<table width=100%><tr>
			<td class='align_left'>%%%search_message%%%</td>
			<td class='align_right'>%%%download_link%%%</td>
		</tr></table>
		</td>
	</tr>
	%%%search_result_set%%%
</table>
</form>
